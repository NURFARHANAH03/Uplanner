public class Event {
    private Student matric_ID;
    private Event event;
    private Location location;
    private Date start_time;
    private Date end_time;

    public Event(Student matric_ID, Event event, Location location, Date start_time, Date end_time) {
        this.matric_ID = matric_ID;
        this.event = event;
        this.location = location;
        this.start_time = start_time;
        this.end_time = end_time;
    }

    public Event() {

    }

    public Student getMatric_ID() {
        return matric_ID;
    }

    public void setMatric_ID(Student matric_ID) {
        this.matric_ID = matric_ID;
    }

    public Event getEvent() {
        return event;
    }

    public void setEvent(Event event) {
        this.event = event;
    }

    public Location getLocation() {
        return location;
    }

    public void setLocation(Location location) {
        this.location = location;
    }

    public Date getStart_time() {
        return start_time;
    }

    public void setStart_time(Date start_time) {
        this.start_time = start_time;
    }

    public Date getEnd_time() {
        return end_time;
    }

    public void setEnd_time(Date end_time) {
        this.end_time = end_time;
    }
}










